    "bluebird": "^3.4.1",
    "bluebird-retry": "^0.8.0",
    "chai": "^3.5.0",
    "connect-timeout": "^1.7.0",
    "debug": "^2.2.0",
    "express": "^4.12.3",
    "mocha": "^2.5.3",
    "mysql": "^2.11.1",
    "npm-build-tools": "^2.2.5",
    "promise-queue": "^2.2.3",
    "promise-throttle": "^0.2.0",
    "request": "^2.74.0",
    "request-promise": "^4.0.2",
    "simple-lru-cache": "0.0.2",
